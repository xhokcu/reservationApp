{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "config/*": ["src/config/*"],
      "screens/*": ["src/screens/*"],
      "navigations/*": ["src/navigations/*"],
      "assets/*": ["assets/*"],
      "types/*": ["src/types/*"],
      "store/*": ["src/store/*"],
      "routes/*": ["src/routes/*"],
      "common/*": ["src/common/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
